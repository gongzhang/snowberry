<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="fatcat.snowberry.core.debug.views"
            name="Snowberry调试视图">
      </category>
      <view
            allowMultiple="false"
            category="fatcat.snowberry.core.debug.views"
            class="fatcat.snowberry.core.debug.views.ConsoleView"
            icon="icons/sample.gif"
            id="fatcat.snowberry.core.debug.views.console"
            name="控制台"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="fatcat.snowberry.core.debug.views"
            class="fatcat.snowberry.core.debug.views.CommandView"
            icon="icons/sample.gif"
            id="fatcat.snowberry.core.debug.views.command"
            name="命令按钮"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="fatcat.snowberry.core.debug.views"
            class="fatcat.snowberry.diagram.debug.views.TagRelatedSearchView"
            icon="icons/sample.gif"
            id="fatcat.snowberry.diagram.debug.views.label.search"
            name="标签（Label）相关资源"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="fatcat.snowberry.core.debug.views"
            class="fatcat.snowberry.dp.debug.views.DesignPatternView"
            icon="icons/sample.gif"
            id="fatcat.snowberry.dp.debug.views.dp"
            name="设计模式"
            restorable="true">
      </view>
      <category
            id="fatcat.snowberry.dp.views"
            name="Snowberry设计模式">
      </category>
      <category
            id="fatcat.snowberry.tag.views"
            name="Snowberry标签视图">
      </category>
      <category
            id="fatcat.snowberry.views"
            name="Snowberry视图">
      </category>
      <view
            allowMultiple="false"
            category="fatcat.snowberry.views"
            class="fatcat.snowberry.tag.views.TagManager"
            icon="icons/sample.gif"
            id="fatcat.snowberry.tag.views.manager"
            name="标签管理器"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="fatcat.snowberry.views"
            class="fatcat.snowberry.dp.views.SchemaView"
            icon="icons/dp_icon.png"
            id="fatcat.snowberry.dp.views.schema"
            name="设计模式"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="fatcat.snowberry.views"
            class="fatcat.snowberry.views.HyperdocView"
            icon="icons/javadoc(2).gif"
            id="fatcat.snowberry.views.hyperdoc"
            name="Hyperdoc"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.viewActions">
      <viewContribution
            id="fatcat.snowberry.tag.views.tagmanager.contribution"
            targetID="fatcat.snowberry.tag.views.manager">
         <action
               class="fatcat.snowberry.tag.actions.AddTagAction"
               icon="icons/add_obj.gif"
               id="fatcat.snowberry.tag.actions.addtag"
               label="添加标签"
               style="push"
               toolbarPath="tag"
               tooltip="添加标签">
         </action>
         <action
               class="fatcat.snowberry.tag.actions.RemoveTagAction"
               icon="icons/delete_obj.gif"
               id="fatcat.snowberry.tag.actions.removetag"
               label="删除标签"
               style="push"
               toolbarPath="tag"
               tooltip="删除标签">
         </action>
      </viewContribution>
      <viewContribution
            id="fatcat.snowberry.dp.views.schema.contribution"
            targetID="fatcat.snowberry.dp.views.schema">
         <action
               class="fatcat.snowberry.dp.actions.AddSchema"
               icon="icons/add_obj.gif"
               id="fatcat.snowberry.dp.views.schema.add"
               label="创建新设计模式"
               style="push"
               toolbarPath="schema"
               tooltip="创建新设计模式">
         </action>
         <action
               class="fatcat.snowberry.dp.actions.RemoveSchema"
               icon="icons/delete_obj.gif"
               id="fatcat.snowberry.dp.views.schema.remove"
               label="删除设计模式"
               style="push"
               toolbarPath="schema"
               tooltip="删除设计模式">
         </action>
      </viewContribution>
      <viewContribution
            id="fatcat.snowberry.views.hyperdoc.contribution"
            targetID="fatcat.snowberry.views.hyperdoc">
      </viewContribution>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="fatcat.snowberry.diagram.DiagramEditor2"
            default="false"
            filenames="*.java"
            icon="icons/snowberry_icon.png"
            id="fatcat.snowberry.diagram.editor2"
            name="Java类图编辑器">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="fatcat.snowberry.core.SnowberryLauncher"></startup>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="fatcat.snowberry.actions"
            label="Snowberry命令集"
            visible="true">
         <menu
               id="fatcat.snowberry.menu"
               label="&amp;Snowberry插件">
            <separator
                  name="about">
            </separator>
         </menu>
         <action
               class="fatcat.snowberry.actions.About"
               icon="icons/snowberry_icon.png"
               id="fatcat.snowberry.actions.test"
               label="关于与Bug反馈..."
               menubarPath="fatcat.snowberry.menu/about"
               style="push">
         </action>
      </actionSet>
   </extension>

</plugin>
