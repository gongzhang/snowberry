package fatcat.gui.util;

import java.awt.image.BufferedImage;

import fatcat.gui.GraphicsX;


public class Image3x3 {
	
	public final BufferedImage image;
	public final int x1, x2, y1, y2, width, height;

	public Image3x3(String res_url, int x1, int x2, int y1, int y2) {
		image = GraphicsX.createImage(res_url);
		this.x1 = x1;
		this.x2 = x2;
		this.y1 = y1;
		this.y2 = y2;
		this.width = image.getWidth();
		this.height = image.getHeight();
	}

}
